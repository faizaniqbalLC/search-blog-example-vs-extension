{
  "name": "fds-search-blog",
  "displayName": "Blogs for Javascript",
  "description": "This extension is for searching blog in VS Code related to Javascript React, React Hooks, Custom Hooks, and much more.",
  "publisher": "FizanDevSimplified",
  "version": "1.1.3",
  "icon": "assets/logo.png",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "search-blog-example.searchBlogExample",
        "title": "FDS search blogs"
      }
    ],
    "keybindings": [
      {
        "command": "search-blog-example.searchBlogExample",
        "key": "ctrl+alt+f",
        "mac": "ctrl+alt+f"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "keywords": [
    "search blog Javascript",
    "React  hooks localstorage"
  ],
  "author": "Muhammad Fizan Iqbal <se.fizaniqbal2@gmail.com",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/faizaniqbalLC/search-blog-example-vs-extension.git"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.77.0",
    "@vscode/test-electron": "^2.3.0",
    "eslint": "^8.36.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "fast-xml-parser": "^4.1.3"
  }
}
